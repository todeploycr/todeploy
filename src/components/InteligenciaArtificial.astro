<script>

    document.addEventListener('DOMContentLoaded', () => {
    const tarjetas: NodeListOf<HTMLElement> = document.querySelectorAll('.ia-caja-item');
    
    tarjetas.forEach((tarjeta: HTMLElement) => {
        tarjeta.addEventListener('click', function(this: HTMLElement, e: Event) {
            e.stopPropagation();
            
            // Quitar clase 'girada' de todas las tarjetas excepto la actual
            tarjetas.forEach((t: HTMLElement) => {
                if (t !== this) t.classList.remove('girada');
            });
            
            // Alternar clase 'girada' en la tarjeta clickeada
            this.classList.toggle('girada');
        });
    });
    
    // Cerrar tarjetas al hacer click fuera
    document.addEventListener('click', (e: Event) => {
        const target = e.target as HTMLElement;
        
        if (!target.closest('.ia-caja-item')) {
            tarjetas.forEach((tarjeta: HTMLElement) => {
                tarjeta.classList.remove('girada');
            });
        }
    });
});


</script>